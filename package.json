{
  "name": "ansi-to-vue3",
  "version": "0.1.0",
  "description": "This package convert ANSI escape codes to formatted text output for Vue3. Copy from ansi-to-react.",
  "keywords": [
    "vue3",
    "ansi",
    "ansi-vue",
    "ansi-vue3",
    "ansi-to-vue",
    "ansi-to-vue3"
  ],
  "bugs": {
    "url": "https://github.com/jerrywu001/ansi-to-vue3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerrywu001/ansi-to-vue3.git"
  },
  "license": "MIT",
  "author": "jerrywu001 <57242263@163.com>",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf -rf ./node_modules && rimraf -rf playground/node_modules && rimraf -rf package-lock.json",
    "dev": "concurrently \"npm run build:css-watch\" \"npm run dev -w playground\"",
    "dev:pack": "npm run dev:usepack -w playground",
    "lint": "eslint --ext .ts,.tsx ./src",
    "prepare": "chmod a+x .husky/* && husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc && vue-tsc"
  },
  "dependencies": {
    "anser": "^2.1.1",
    "escape-carriage": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@swc/core": "^1.3.35",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/ui": "^0.28.5",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "bumpp": "^8.2.1",
    "concurrently": "^7.6.0",
    "core-js": "^3.28.0",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "npm": "^9.5.0",
    "rimraf": "^4.1.2",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.1.5"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  }
}
